{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "build": "npm run-script-if-node-version \">=16\" \"tsc\" \"--\" \"echo \\\"Skipping build on Node.js version < 16.\\\"\"",
    "lint": "npm run-script-if-node-version \">=16\" \"eslint --ext .js,.ts .\" \"--\" \"echo \\\"Skipping lint on Node.js version < 16.\\\"\"",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.1",
    "@genkit-ai/google-genai": "^1.2.0",
    "genkit": "^1.1.0",
    "zod": "^3.23.8",
    "firebase-functions-test": "^3.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "run-script-if-node-version": "^1.0.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
